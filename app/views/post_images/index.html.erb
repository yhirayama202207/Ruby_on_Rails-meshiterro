<h1>PostImages#index</h1>
<p>Find me in app/views/post_images/index.html.erb</p>

<% @post_images.each do |post_image| %>
<div>
  <p>投稿ユーザー画像：<%= image_tag post_image.get_image %></p>
  <p>ショップ名：<%= post_image.shop_name %></p>
  <p>説明：<%= post_image.caption %></p>
  <p>ユーザーネーム：<%= post_image.user.name %></p>
</div>
<% end %>
